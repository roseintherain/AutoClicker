<Window x:Class="AutoClicker.Views.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Topmost="true"
        ResizeMode="CanMinimize"
        Height="150" Width="250"
        Icon="{StaticResource AboutIcon}">
    <DockPanel>
        <TextBlock VerticalAlignment="Center" TextAlignment="Center"><Run x:Name="AboutInformationText"/><LineBreak/><Run x:Name="AuthorText" Text="Created by Chris"/><LineBreak/><LineBreak/></TextBlock>
    </DockPanel>
</Window>
